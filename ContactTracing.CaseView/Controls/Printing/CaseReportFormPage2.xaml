                        <TextBlock Margin="5, 0, 5, 0" Grid.Row="0" Grid.Column="3" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Text="{x:Static p:Resources.PrintedFormSourceColHeaderVillage}" />
                        <TextBlock Margin="5, 0, 5, 0" Grid.Row="0" Grid.Column="4" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Text="{x:Static p:Resources.PrintedFormSourceColHeaderDistrict}" />
                        <TextBlock Margin="5, 0, 5, 0" Grid.Row="0" Grid.Column="5" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Text="{x:Static p:Resources.PrintedFormSourceColHeaderDeadAlive}" />
                        <TextBlock Margin="5, 0, 5, 0" Grid.Row="0" Grid.Column="6" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Text="{x:Static p:Resources.PrintedFormSourceColHeaderContactTypes}" />

                        <StackPanel Margin="5, 1, 5, 1" Orientation="Horizontal" Grid.Column="2" Grid.Row="1">
                            <printing:DateDisplay x:Name="dateHospFrom1" DayMonthYearVisibility="Collapsed" />
                            <TextBlock Margin="3, 0, 3, 0">-</TextBlock>
                            <printing:DateDisplay x:Name="dateHospTo1" />
                        </StackPanel>

                        <StackPanel Margin="5, 1, 5, 1" Orientation="Horizontal" Grid.Column="2" Grid.Row="2">
                            <printing:DateDisplay x:Name="dateHospFrom2" DayMonthYearVisibility="Collapsed" />
                            <TextBlock Margin="3, 0, 3, 0">-</TextBlock>
                            <printing:DateDisplay x:Name="dateHospTo2" />
                        </StackPanel>

                        <StackPanel Margin="5, 1, 5, 1" Orientation="Horizontal" Grid.Column="2" Grid.Row="3">
                            <printing:DateDisplay x:Name="dateHospFrom3" DayMonthYearVisibility="Collapsed" />
                            <TextBlock Margin="3, 0, 3, 0">-</TextBlock>
                            <printing:DateDisplay x:Name="dateHospTo3" />
                        </StackPanel>

                        <TextBlock FontSize="8pt" Grid.Row="1" Grid.Column="0" x:Name="tblockNameSoureCase1" Margin="5, 0, 5, 0" HorizontalAlignment="Left" VerticalAlignment="Center" />
                        <TextBlock FontSize="8pt" Grid.Row="2" Grid.Column="0" x:Name="tblockNameSoureCase2" Margin="5, 0, 5, 0" HorizontalAlignment="Left" VerticalAlignment="Center" />
                        <TextBlock FontSize="8pt" Grid.Row="3" Grid.Column="0" x:Name="tblockNameSoureCase3" Margin="5, 0, 5, 0" HorizontalAlignment="Left" VerticalAlignment="Center" />

                        <TextBlock FontSize="8pt" Grid.Row="1" Grid.Column="1" x:Name="tblockRelationToPatient1" Margin="5, 0, 5, 0" HorizontalAlignment="Left" VerticalAlignment="Center" />
                        <TextBlock FontSize="8pt" Grid.Row="2" Grid.Column="1" x:Name="tblockRelationToPatient2" Margin="5, 0, 5, 0" HorizontalAlignment="Left" VerticalAlignment="Center" />
                        <TextBlock FontSize="8pt" Grid.Row="3" Grid.Column="1" x:Name="tblockRelationToPatient3" Margin="5, 0, 5, 0" HorizontalAlignment="Left" VerticalAlignment="Center" />

                        <TextBlock FontSize="8pt" Grid.Row="1" Grid.Column="3" x:Name="tblockVillage1" Margin="5, 0, 5, 0" HorizontalAlignment="Left" VerticalAlignment="Center" />
                        <TextBlock FontSize="8pt" Grid.Row="2" Grid.Column="3" x:Name="tblockVillage2" Margin="5, 0, 5, 0" HorizontalAlignment="Left" VerticalAlignment="Center" />
                        <TextBlock FontSize="8pt" Grid.Row="3" Grid.Column="3" x:Name="tblockVillage3" Margin="5, 0, 5, 0" HorizontalAlignment="Left" VerticalAlignment="Center" />

                        <TextBlock FontSize="8pt" Grid.Row="1" Grid.Column="4" x:Name="tblockDistrict1" Margin="5, 0, 5, 0" HorizontalAlignment="Left" VerticalAlignment="Center" />
                        <TextBlock FontSize="8pt" Grid.Row="2" Grid.Column="4" x:Name="tblockDistrict2" Margin="5, 0, 5, 0" HorizontalAlignment="Left" VerticalAlignment="Center" />
                        <TextBlock FontSize="8pt" Grid.Row="3" Grid.Column="4" x:Name="tblockDistrict3" Margin="5, 0, 5, 0" HorizontalAlignment="Left" VerticalAlignment="Center" />


                        <TextBlock Grid.Row="1" Grid.Column="6" x:Name="tblockContactTypes1" Margin="5, 0, 5, 0" HorizontalAlignment="Left" VerticalAlignment="Center" />
                        <TextBlock Grid.Row="2" Grid.Column="6" x:Name="tblockContactTypes2" Margin="5, 0, 5, 0" HorizontalAlignment="Left" VerticalAlignment="Center" />
                        <TextBlock Grid.Row="3" Grid.Column="6" x:Name="tblockContactTypes3" Margin="5, 0, 5, 0" HorizontalAlignment="Left" VerticalAlignment="Center" />

                        <StackPanel Orientation="Vertical"  Grid.Row="1" Grid.Column="5" Margin="2, 0, 2, 0">
                            <StackPanel Orientation="Horizontal">
                                <Border BorderThickness="1" BorderBrush="Black" Width="11" Height="11">
                                    <TextBlock Visibility="Collapsed" x:Name="tblockAlive1" FontSize="7pt" HorizontalAlignment="Center" VerticalAlignment="Center">√</TextBlock>
                                </Border>
                                <TextBlock FontSize="8pt" Text="{x:Static p:Resources.Alive}"></TextBlock>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Border BorderThickness="1" BorderBrush="Black" Width="11" Height="11">
                                    <TextBlock Visibility="Collapsed" x:Name="tblockDead1" FontSize="7pt" HorizontalAlignment="Center" VerticalAlignment="Center">√</TextBlock>
                                </Border>
                                <TextBlock FontSize="8pt" Text="{x:Static p:Resources.Dead}"></TextBlock>
                                <TextBlock Margin="4, 0, 0, 0" FontSize="8pt" Text="{x:Static p:Resources.PrintedFormSourceDiedOn}"></TextBlock>
                                <TextBlock x:Name="tblockDateDeath1" FontSize="8pt" Margin="4, 0, 0, 0" Text=""></TextBlock>
                            </StackPanel>
                        </StackPanel>

                        <StackPanel Orientation="Vertical"  Grid.Row="2" Grid.Column="5" Margin="2, 0, 2, 0">
                            <StackPanel Orientation="Horizontal">
                                <Border BorderThickness="1" BorderBrush="Black" Width="11" Height="11">
                                    <TextBlock Visibility="Collapsed" x:Name="tblockAlive2" FontSize="7pt" HorizontalAlignment="Center" VerticalAlignment="Center">√</TextBlock>
                                </Border>
                                <TextBlock FontSize="8pt" Text="{x:Static p:Resources.Alive}"></TextBlock>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Border BorderThickness="1" BorderBrush="Black" Width="11" Height="11">
                                    <TextBlock Visibility="Collapsed" x:Name="tblockDead2" FontSize="7pt" HorizontalAlignment="Center" VerticalAlignment="Center">√</TextBlock>
                                </Border>
                                <TextBlock FontSize="8pt" Text="{x:Static p:Resources.Dead}"></TextBlock>
                                <TextBlock Margin="4, 0, 0, 0" FontSize="8pt" Text="{x:Static p:Resources.PrintedFormSourceDiedOn}"></TextBlock>
                                <TextBlock x:Name="tblockDateDeath2" FontSize="8pt" Margin="4, 0, 0, 0" Text=""></TextBlock>
                            </StackPanel>
                        </StackPanel>

                        <StackPanel Orientation="Vertical"  Grid.Row="3" Grid.Column="5" Margin="2, 0, 2, 0">
                            <StackPanel Orientation="Horizontal">
                                <Border BorderThickness="1" BorderBrush="Black" Width="11" Height="11">
                                    <TextBlock Visibility="Collapsed" x:Name="tblockAlive3" FontSize="7pt" HorizontalAlignment="Center" VerticalAlignment="Center">√</TextBlock>
                                </Border>
                                <TextBlock FontSize="8pt" Text="{x:Static p:Resources.Alive}"></TextBlock>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Border BorderThickness="1" BorderBrush="Black" Width="11" Height="11">
                                    <TextBlock Visibility="Collapsed" x:Name="tblockDead3" FontSize="7pt" HorizontalAlignment="Center" VerticalAlignment="Center">√</TextBlock>
                                </Border>
                                <TextBlock FontSize="8pt" Text="{x:Static p:Resources.Dead}"></TextBlock>
                                <TextBlock Margin="4, 0, 0, 0" FontSize="8pt" Text="{x:Static p:Resources.PrintedFormSourceDiedOn}"></TextBlock>
                                <TextBlock x:Name="tblockDateDeath3" FontSize="8pt" Margin="4, 0, 0, 0" Text=""></TextBlock>
                            </StackPanel>
                        </StackPanel>
                    </Grid>

                    <StackPanel HorizontalAlignment="Center" Orientation="Horizontal" Margin="2">
                        <TextBlock HorizontalAlignment="Center" Margin="-100, 0, 3, 0" FontWeight="Bold" Text="{x:Static p:Resources.PrintedFormContactTypeLegendTitle}" />
                        <Grid Margin="2, 0, 0, 0" HorizontalAlignment="Center">
                            <Rectangle Grid.RowSpan="100" Grid.ColumnSpan="100" Stroke="Black" Fill="Transparent" StrokeThickness="1" StrokeDashArray="0.5 1.0 0.3" />
                            <StackPanel Margin="4, 0, 4, 0">
                                <TextBlock Text="{x:Static p:Resources.PrintedFormContactLegend1}" />
                                <TextBlock Text="{x:Static p:Resources.PrintedFormContactLegend2}" />
                                <TextBlock Text="{x:Static p:Resources.PrintedFormContactLegend3}" />
                                <TextBlock Text="{x:Static p:Resources.PrintedFormContactLegend4}" />
                            </StackPanel>
                        </Grid>
                    </StackPanel>


                    <StackPanel Orientation="Horizontal">
                        <TextBlock 
                                            FontWeight="Bold" 
                                            Margin="5, 0, 5, 0"
                        Text="{x:Static p:Resources.PrintedFormQ2}"
                        >

                        </TextBlock>
                        <CheckBox Margin="5, 0, 5, 0" VerticalAlignment="Center" x:Name="checkboxAttendFuneralYes" Content="{x:Static p:Resources.Yes}" />
                        <CheckBox Margin="5, 0, 5, 0" VerticalAlignment="Center" x:Name="checkboxAttendFuneralNo" Content="{x:Static p:Resources.No}" />
                        <CheckBox Margin="5, 0, 5, 0" VerticalAlignment="Center" x:Name="checkboxAttendFuneralUnk" Content="{x:Static p:Resources.PrintedFormUnknown}" />
                    </StackPanel>

                    <TextBlock Margin="15, 0, 0, 0" FontStyle="Italic" Text="{x:Static p:Resources.PrintedFormIfYesFuneral}"/>


                    <Grid Margin="5, 0, 5, 0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                        </Grid.RowDefinitions>

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>

                        <Border Grid.Row="0" Grid.Column="0" BorderBrush="Black" BorderThickness="1,1,0,0" />
                        <Border Grid.Row="1" Grid.Column="0" BorderBrush="Black" BorderThickness="1,1,0,0" />
                        <Border Grid.Row="2" Grid.Column="0" BorderBrush="Black" BorderThickness="1,1,0,0" />


                        <Border Grid.Row="0" Grid.Column="1" BorderBrush="Black" BorderThickness="1,1,0,0" />
                        <Border Grid.Row="1" Grid.Column="1" BorderBrush="Black" BorderThickness="1,1,0,0" />
                        <Border Grid.Row="2" Grid.Column="1" BorderBrush="Black" BorderThickness="1,1,0,0" />


                        <Border Grid.Row="0" Grid.Column="2" BorderBrush="Black" BorderThickness="1,1,0,0" />
                        <Border Grid.Row="1" Grid.Column="2" BorderBrush="Black" BorderThickness="1,1,0,0" />
                        <Border Grid.Row="2" Grid.Column="2" BorderBrush="Black" BorderThickness="1,1,0,0" />


                        <Border Grid.Row="0" Grid.Column="3" BorderBrush="Black" BorderThickness="1,1,0,0" />
                        <Border Grid.Row="1" Grid.Column="3" BorderBrush="Black" BorderThickness="1,1,0,0" />
                        <Border Grid.Row="2" Grid.Column="3" BorderBrush="Black" BorderThickness="1,1,0,0" />


                        <Border Grid.Row="0" Grid.Column="4" BorderBrush="Black" BorderThickness="1,1,0,0" />
                        <Border Grid.Row="1" Grid.Column="4" BorderBrush="Black" BorderThickness="1,1,0,0" />
                        <Border Grid.Row="2" Grid.Column="4" BorderBrush="Black" BorderThickness="1,1,0,0" />


                        <Border Grid.Row="0" Grid.Column="5" BorderBrush="Black" BorderThickness="1,1,1,0" />
                        <Border Grid.Row="1" Grid.Column="5" BorderBrush="Black" BorderThickness="1,1,1,0" />
                        <Border Grid.Row="2" Grid.Column="5" BorderBrush="Black" BorderThickness="1,1,1,1" />


                        <TextBlock Margin="5, 0, 5, 0" Grid.Row="0" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Text="{x:Static p:Resources.PrintedFormFuneralColHeaderName}" />
                        <TextBlock Margin="5, 0, 5, 0" Grid.Row="0" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Text="{x:Static p:Resources.PrintedFormFuneralColHeaderRelation}" />
                        <TextBlock Margin="5, 0, 5, 0" Grid.Row="0" Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Text="{x:Static p:Resources.PrintedFormFuneralColHeaderDates}" />
                        <TextBlock Margin="5, 0, 5, 0" Grid.Row="0" Grid.Column="3" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Text="{x:Static p:Resources.PrintedFormSourceColHeaderVillage}" />
                        <TextBlock Margin="5, 0, 5, 0" Grid.Row="0" Grid.Column="4" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Text="{x:Static p:Resources.PrintedFormSourceColHeaderDistrict}" />
                        <TextBlock Margin="5, 0, 5, 0" Grid.Row="0" Grid.Column="5" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Text="{x:Static p:Resources.PrintedFormFuneralColHeaderTouchBody}" />


                        <StackPanel Margin="5, 1, 5, 1" Orientation="Horizontal" Grid.Column="2" Grid.Row="1">
                            <printing:DateDisplay x:Name="dateFuneralFrom1" DayMonthYearVisibility="Collapsed" />
                            <TextBlock Margin="5, 0, 5, 0">-</TextBlock>
                            <printing:DateDisplay x:Name="dateFuneralTo1" />
