                            <printing:DateDisplay x:Name="dateFuneralFrom2" DayMonthYearVisibility="Collapsed" />
                            <TextBlock Margin="5, 0, 5, 0">-</TextBlock>
                            <printing:DateDisplay x:Name="dateFuneralTo2" />
                        </StackPanel>

                        <TextBlock Grid.Row="1" Grid.Column="0" x:Name="tblockName1" Margin="5, 0, 5, 0" HorizontalAlignment="Left" VerticalAlignment="Center" />
                        <TextBlock Grid.Row="2" Grid.Column="0" x:Name="tblockName2" Margin="5, 0, 5, 0" HorizontalAlignment="Left" VerticalAlignment="Center" />

                        <TextBlock Grid.Row="1" Grid.Column="1" x:Name="tblockRelationToPatientFuneral1" Margin="5, 0, 5, 0" HorizontalAlignment="Left" VerticalAlignment="Center" />
